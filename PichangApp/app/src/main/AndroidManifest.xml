<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.miguelch96.pichangapp">
<uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.CanchaActivity" />
        <activity android:name=".activities.SignInActivity" />
        <activity android:name=".activities.EquipoActivity">
        <meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".activities.MainActivity" />
        </activity>
        <activity android:name=".dialogs.equipo.IntegrantesDialog" />
        <activity android:name=".dialogs.equipo.SkillsDialog" />
        <activity android:name=".dialogs.equipo.ScoresDialog"></activity>
    </application>

</manifest>